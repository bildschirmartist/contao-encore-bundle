{% for entry in jsEntries %}
    {{ encore_entry_script_tags(entry) }}
{% endfor %}

{% if addEncoreBabelPolyfill|default() %}
    <script type="text/javascript">
        if(/MSIE \d|Trident.*rv:/.test(navigator.userAgent))
            document.write('<script src="{{ asset(encorePublicPath ~ '/' ~ encoreBabelPolyfillEntryName ~ '.js') }}"><\/script>');
    </script>
{% endif %}